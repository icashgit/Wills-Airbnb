          "_customHTML": "<section data-bs-version=\"5.1\" class=\"pricing-table1 cleanm4_pricing-table1\" group=\"Pricing Tables\" data-bg-video=\"{{bg.type == 'video'&&bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax, 'mbr-fullscreen': fullScreen}\">\n\n\t<mbr-parameters>\n\t\t\n\t\t<header>Size</header>\n\t\t<input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n\t\t<input inline type=\"range\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"1\" condition=\"!fullScreen\">\n\t\t<input inline type=\"range\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"6\" condition=\"!fullScreen\">\n\t\t<select title=\"Items Count\" name=\"itemsCount\">\n\t\t\t<option value=\"1\">1</option>\n\t\t\t<option value=\"2\">2</option>\n\t\t\t<option value=\"3\" selected>3</option>\n\t\t\t<option value=\"4\">4</option>\n\t\t</select>\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Main Title\" name=\"showMainTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n\t\t<input type=\"checkbox\" title=\"Text\" name=\"showText\">\n\t\t<input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked>\n\t\t<input type=\"checkbox\" title=\"Price\" name=\"showPrice\" checked>\n\t\t<input type=\"checkbox\" title=\"Check Mark\" name=\"showCounter\">\n\t\t<input type=\"color\" title=\"Check Mark Color\" name=\"counterColor\" value=\"#ffffff\" condition=\"showCounter\">\n\t\t<input type=\"color\" title=\"Card Color\" name=\"CardBg\" value=\"#42d1ab\">\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n\t\t\t<input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n\t\t\t<input type=\"video\" title=\"Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n\t\t</fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n\t\t<input type=\"checkbox\" title=\"Gradient Bg\" name=\"isGradientBg\" checked>\n\t\t<input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#ffffff\" condition=\"bg.type=='color' && isGradientBg\" selected>\n\t\t<input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n\t\t<input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n\t\t<input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay&&bg.type !== 'color'\">\n\t\t\n\t</mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n\n\t<div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n\t</div>\n\n\t<div class=\"container\">\n\t\t<div class=\"head_info\">\n\t\t\t<h1 class=\"mbr-section-title mbr-fonts-style mbr-bold\" data-app-selector=\".head_info .mbr-section-title\" mbr-if=\"showMainTitle\" mbr-theme-style=\"display-2\">Sample</h1>\n          \n\t\t\t<h2 class=\"mbr-subtitle mbr-fonts-style mbr-semibold\" data-app-selector=\".head_info .mbr-subtitle\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-4\">OUR PRICING PLANS</h2>\n\t\t\t<p class=\"mbr-section-text mbr-fonts-style mbr-regular\" data-app-selector=\".head_info .mbr-section-text\" mbr-if=\"showText\" mbr-theme-style=\"display-7\">\n\t\t\t\tCondimentum vitae sapien pellentesque habitant morbi tristique senectus.\n\t\t\t\tNibh cras pulvinar mattis nunc.</p>\n\t\t</div>\n\n\t\t<div class=\"media-container-row\">\n\n\t\t\t<div class=\"plan col-12 mx-3 my-2 justify-content-center\" mbr-if=\"itemsCount > 0\" mbr-class=\"{\n                'col-lg-4': itemsCount == 3,\n                'col-lg-6': itemsCount == 2,\n                'col-lg-3': itemsCount == 4}\">\n\n\t\t\t\t<h3 class=\"mbr-fonts-style mbr-semibold plan-title\" mbr-theme-style=\"display-5\" data-app-selector=\".plan .plan-title\">\n\t\t\t\t\tBasic Part-time</h3>\n\t\t\t\t<div class=\"icon_box\">\n\t\t\t\t\t<span mbr-icon class=\"mbr-iconfont ico1 icon54-v3-rocket-backpack\" style=\"color: rgb(255, 255, 255); fill: rgb(255, 255, 255);\"></span>\n\t\t\t\t</div>\n\t\t\t\t<div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn align-center\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\" data-app-selector=\".plan .mbr-section-btn .btn\"><a href=\"https://www.paypal.com/cgi-bin/webscr?cmd=_xclick-subscriptions&item_name=Kampi%20Cloud%20Plan%3A%20Basic&item_number=Kampi%20Cloud%20Plan%3A%20Full-Time&currency_code=USD&a3=1&p3=12&t3=M&src=1\" class=\"btn btn-lg btn-info\" data-app-selector=\".plan .mbr-section-btn .btn\" data-app-placeholder=\"Type Text\" target=\"_blank\">Subscribe</a></div>\n\t\t\t\t<div class=\"plan-list align-center\">\n\t\t\t\t\t<ul mbr-text class=\"mbr-fonts-style mbr-regular list-group list-group-flush\" mbr-theme-style=\"display-4\" data-app-selector=\".plan .plan-list .list-group, .mbr-section-btn\">\n\t\t\t\t\t\t<li class=\"list-group-item\">Inbound Calls</li>\n\t\t\t\t\t\t<li class=\"list-group-item\">\n\t\t\t\t\t\t\tAdmin Work</li>\n\t\t\t\t\t\t<li class=\"list-group-item\">&nbsp;Client Transfers</li>\n\t\t\t\t\t\t<li class=\"list-group-item\">\n\t\t\t\t\t\t\t20 Hours/week</li><br><br><li class=\"list-group-item\"><br></li>\n\t\t\t\t\t</ul>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"plan-price px-3\" condition=\"showPrice\">\n\t\t\t\t\t<p class=\"mbr-text mbr-fonts-style mbr-bold\" data-app-selector=\".plan .plan-price .mbr-text\" mbr-if=\"showPrice\" mbr-theme-style=\"display-2\">\n\t\t\t\t\t\t$995</p>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t\t<div class=\"plan 