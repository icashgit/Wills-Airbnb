                  }
                },
                ".btn-primary": {
                  "&:hover": {
                    "background-color": "#28303f !important",
                    "color": "@successColor !important"
                  }
                }
              },
              "&:hover": {
                ".mbr-iconfont": {
                  "-webkit-animation-name": "pulse-shrink",
